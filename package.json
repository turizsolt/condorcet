{
  "name": "condorcet",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/turizsolt/condorcet.git",
  "author": "turizsolt <turi.zsolt.m@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clear && yarn lint && yarn test",
    "build": "webpack",
    "start": "webpack && node build/index.js",
    "start:dev": "NODE_ENV=development webpack",
    "run:dev": "NODE_ENV=development nodemon build/index.js",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} --fix --format verbose",
    "clear": "rm -r build/* || true",

    "test": "nyc mocha -r ts-node/register test/**/*.{ts,tsx}",
    "posttest": "nyc report --reporter=json"
  },
  "devDependencies": {
    "@types/chai": "^4.2.6",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
